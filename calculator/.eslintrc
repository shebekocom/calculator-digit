{
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "es6": true,
    "jest": true,
    "node": true
  },
  "extends": [
    "airbnb",
    "prettier",
    "prettier/react",
    "plugin:prettier/recommended",
    "plugin:react/recommended",
    "eslint-config-prettier"
  ],
  "rules": {
    "prettier/prettier": "error",
    "no-console":"off",
    "strict": 0,
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
    "import/no-unresolved": "off",
    "import/extensions": "off"

  },
  "plugins": [
    "prettier",
    "react-hooks"
  ]
}
